<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="DefaultSelectionBrush" Color="{StaticResource DefaultSelectionColor}" />
    <SolidColorBrush x:Key="HoverSelectionBrush" Color="{StaticResource HoverSelectionColor}" />
    <SolidColorBrush x:Key="PressedSelectionBrush" Color="{StaticResource PressedSelectionColor}" />
    <SolidColorBrush x:Key="DisabledSelectionBrush" Color="{StaticResource DisabledSelectionColor}" />

    <SolidColorBrush x:Key="ThemeDarkBrush" Color="{StaticResource ThemeDarkColor}" />
    <SolidColorBrush x:Key="ThemeMediumBrush" Color="{StaticResource ThemeMediumColor}" />
	<SolidColorBrush x:Key="ThemeLightBrush" Color="{StaticResource ThemeLightColor}" />
    <SolidColorBrush x:Key="LightBrush" Color="{StaticResource ThemeLightColor}" />
    
    <LinearGradientBrush x:Key="ThemeGradientBrush" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource ThemeMediumColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource ThemeLightColor}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="ButtonCyanBrush" StartPoint="0,0" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource ButtonCyanLightColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource ButtonCyanDarkColor}" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonBlueBrush" StartPoint="0,0" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource ButtonBlueLightColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource ButtonBlueDarkColor}" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonPurpleBrush" StartPoint="0,0" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource ButtonPurpleLightColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource ButtonPurpleDarkColor}" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonGreenBrush" StartPoint="0,0" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource ButtonGreenLightColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource ButtonGreenDarkColor}" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonOrangeBrush" StartPoint="0,0" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource ButtonOrangeLightColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource ButtonOrangeDarkColor}" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ButtonGrayBrush" StartPoint="0,0" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource ButtonGrayLightColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource ButtonGrayDarkColor}" />
    </LinearGradientBrush>


    <LinearGradientBrush x:Key="BackGroundColor" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource BackGroundDarkColor}" />
        <GradientStop Offset="0.5" Color="{DynamicResource BackGroundLightColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource BackGroundDarkColor}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="DataGridRowSelectedRowBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{DynamicResource PressedSelectionColor}" />
        <GradientStop Offset="0.375" Color="{DynamicResource HoverSelectionColor}" />
        <GradientStop Offset="0.375" Color="{DynamicResource DefaultSelectionColor}" />
        <GradientStop Offset="1" Color="{DynamicResource DisabledSelectionColor}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="SelectedItemBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{StaticResource HoverSelectionColor}" />
        <GradientStop Offset="1" Color="{StaticResource DefaultSelectionColor}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HoverItemBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{StaticResource PressedSelectionColor}" />
        <GradientStop Offset="1" Color="{StaticResource HoverSelectionColor}" />
    </LinearGradientBrush>

    <!--  Button  -->
    <RadialGradientBrush x:Key="ButtonNwormalBrush" Center="0.5,0.1" GradientOrigin="0.5,0.1" RadiusX="0.5" RadiusY="0.8" >
        <GradientStop Offset="0.0" Color="{DynamicResource ThemeMediumColor}" />
        <GradientStop Offset="0.4" Color="{DynamicResource ThemeMediumColor}" />
        <GradientStop Offset="0.5" Color="{DynamicResource ThemeLightColor}" />
        <GradientStop Offset="0.6" Color="{DynamicResource ThemeMediumColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource ThemeMediumColor}" />
    </RadialGradientBrush>

    <RadialGradientBrush x:Key="ButtonNormalBrush" RadiusX="6" RadiusY="2" Center="0.5,0.5" GradientOrigin="0.5,0.5">
        <RadialGradientBrush.GradientStops>
            <GradientStop Color="{DynamicResource ThemeMediumColor}" Offset="0"/>
            <GradientStop Color="{DynamicResource ThemeLightColor}" Offset="1"/>
        </RadialGradientBrush.GradientStops>
        <RadialGradientBrush.RelativeTransform>
            <TransformGroup>
                <RotateTransform CenterX="0.506667" CenterY="0.439999" Angle="90"/>
            </TransformGroup>
        </RadialGradientBrush.RelativeTransform>
    </RadialGradientBrush>

    <LinearGradientBrush x:Key="ButtonHowerBrush"  StartPoint="0.5,0.039" EndPoint="0.5,0.976">
        <GradientStop Offset="0" Color="#7F595959" />
        <GradientStop Offset="1" Color="#19FFFFFF" />
    </LinearGradientBrush>

    <!--  TabControl Header  -->
    <SolidColorBrush x:Key="DarkBackGroundBrush" Color="{StaticResource BackGroundDarkColor}" />
    <SolidColorBrush x:Key="LightBackGroundBrush" Color="{StaticResource BackGroundContentColor}" />
    <SolidColorBrush x:Key="ShadeBrush" Color="{StaticResource ThemeDarkColor}" />


    <!--  TabItemHeader background  -->
    <LinearGradientBrush x:Key="HeaderActiveBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{DynamicResource HeaderActiveLightColor}" />
        <GradientStop Offset="0.679" Color="{DynamicResource HeaderActiveDarkColor}" />
    </LinearGradientBrush>

    <!--  TabItemContent background (drawing)  -->
    <DrawingBrush x:Key="ContentBackgroundDrawing"
                  TileMode="Tile"
                  Viewport="0,0,0.0025,0.0025">
        <DrawingBrush.Drawing>
            <DrawingGroup>
                <GeometryDrawing Brush="#464E5B">
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry Rect="0,0,10,10" />
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>

                <GeometryDrawing>
                    <GeometryDrawing.Geometry>
                        <GeometryGroup>
                            <RectangleGeometry Rect="0,0,5,5" />
                            <RectangleGeometry Rect="5,5,5,5" />
                        </GeometryGroup>
                    </GeometryDrawing.Geometry>
                    <GeometryDrawing.Brush>
                        <LinearGradientBrush>
                            <GradientStop Offset="0.0" Color="#16202A" />
                            <GradientStop Offset="1.0" Color="#121923" />
                        </LinearGradientBrush>
                    </GeometryDrawing.Brush>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>

    <!--  TabItemContent background (gradient)  -->
    <LinearGradientBrush x:Key="ContentBackgroundGradient" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Offset="0.0" Color="{DynamicResource BackGroundDarkColor}" />
        <GradientStop Offset="0.5" Color="{DynamicResource BackGroundLightColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource BackGroundDarkColor}" />

    </LinearGradientBrush>

    <!--  Background for the footer of the dialog box  -->
    <SolidColorBrush x:Key="DialogFooterBrush" Color="#FFE6E8E9" />

    <!--  Border for the footer of the dialog box  -->
    <SolidColorBrush x:Key="DialogFooterBorderBrush" Color="#FFC2C2C2" />

    <!--  Headline text color  -->
    <SolidColorBrush x:Key="HeadlineTextBrush" Color="#FF407FC4" />

    <!--  Tooltip balloon brush  -->
    <DrawingBrush x:Key="ToolTipBalloonBrush"
                  Viewbox="0,0,1,1"
                  ViewboxUnits="RelativeToBoundingBox">
        <DrawingBrush.Drawing>
            <GeometryDrawing Geometry="M24.500002,0.5 C48.312454,24.312502 54.124943,30.406252 55.542908,31.964842 L55.574356,32.000005 471.5,32.000005 C473.70914,32.000002 475.5,33.790863 475.5,36.000003 L475.5,115 C475.5,117.20914 473.70914,119 471.5,119 L4.4999981,119 C2.2908609,119 0.5,117.20914 0.5,115 L0.5,36.000003 C0.5,33.790863 2.2908609,32.000002 4.4999981,32.000005 L24.500002,32.000005 C24.5,32.000002 24.5,0.50000191 24.500002,0.5 z">
                <GeometryDrawing.Brush>
                    <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                        <GradientStop Offset="0" Color="White" />
                        <GradientStop Offset="1" Color="#FFf6f6f6" />
                    </LinearGradientBrush>
                </GeometryDrawing.Brush>
                <GeometryDrawing.Pen>
                    <Pen Brush="#FFC5C3C3"
                         DashCap="Flat"
                         EndLineCap="Flat"
                         LineJoin="Miter"
                         MiterLimit="10"
                         StartLineCap="Flat"
                         Thickness="1" />
                </GeometryDrawing.Pen>
            </GeometryDrawing>
        </DrawingBrush.Drawing>
    </DrawingBrush>

    <!--  Main window border brush  -->
    <SolidColorBrush x:Key="MainWindowBorderBrush" Color="#ACA3A3A3" />

    <!--  Main window background brush  -->
    <LinearGradientBrush x:Key="MainWindowBackgroundBrush" SpreadMethod="Reflect" StartPoint="0.5,0" EndPoint="0.5,0.5">
        <GradientStop Color="#FFA3D7FF" />
        <GradientStop Offset="1" Color="#FFF2F9FF" />
    </LinearGradientBrush>

    <!--  Main window title bar's separator's brush  -->
    <LinearGradientBrush x:Key="MainWindowTitleBarSeparator" SpreadMethod="Reflect" EndPoint="0.5,0.25">
        <GradientStop Color="#FFA3D7FF" />
        <GradientStop Offset="1" Color="#33FFFFFF" />
    </LinearGradientBrush>

    <!--  Normal background for flat button  -->
    <LinearGradientBrush x:Key="FlatButtonNormalBackground" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#F3F3F3" />
        <GradientStop Offset="0.5" Color="#EBEBEB" />
        <GradientStop Offset="0.5" Color="#DDDDDD" />
        <GradientStop Offset="1" Color="#CDCDCD" />
    </LinearGradientBrush>

    <!--  Normal border brush for flat button  -->
    <SolidColorBrush x:Key="FlatButtonNormalBorder" Color="#FF707070" />

    <!--  Hover background for flat button  -->
    <SolidColorBrush x:Key="FlatButtonHoverBackgroundBrush" Color="#FFC5E4FC" />

    <!--  Hover border brush for flat button  -->
    <SolidColorBrush x:Key="FlatButtonHoverBorderBrush" Color="#FFA5BCF8" />

    <!--  Disabled text color for flat button  -->
    <SolidColorBrush x:Key="FlatButtonDisabledForegroundBrush" Color="#ADADAD" />

    <!--  Backgound brush for dock pane header  -->
    <SolidColorBrush x:Key="DockPaneBackgroundBrush" Color="#FF293955" />

    <!--  Border brush for dock pane header  -->
    <SolidColorBrush x:Key="DockPaneBorderBrush" Color="#FF293955" />

    <!--  Dock pane foreground brush  -->
    <SolidColorBrush x:Key="DockPaneHeaderForegroundBrush" Color="#FFced4dd" />

    <!--  Dock pane hover background brush  -->
    <SolidColorBrush x:Key="DockPaneHoverBackgroundBrush" Color="#FF86A4DA" />

    <!--  Fill color for dock illustration  -->
    <SolidColorBrush x:Key="DockIllustrationBrush" Color="#3F293955" />

    <!--  Active TabItem Background  -->
    <SolidColorBrush x:Key="ActiveTabItemBackground" Color="#FFfddf8a" />

    <!--  Active TabItem Close Button Hover Background  -->
    <SolidColorBrush x:Key="ActiveTabItemCloseButtonHoverBackground" Color="#FFfcf2df" />

    <LinearGradientBrush x:Key="BoldTextBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0.0" Color="{DynamicResource LightTextColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource DarkTextColor}" />

    </LinearGradientBrush>


    <SolidColorBrush x:Key="TextBrush" Color="#ECEBEE" />
    <SolidColorBrush x:Key="NormalBrush" Color="{StaticResource MainColor}" />
    <SolidColorBrush x:Key="NormalBorderBrush" Color="#FF333333" />
    <SolidColorBrush x:Key="HoverBrush" Color="{StaticResource HoverColor}" />
    <SolidColorBrush x:Key="GlyphBrush" Color="#FF008181" />

    <SolidColorBrush x:Key="DarkTextBrush" Color="{StaticResource DarkTextColor}" />
    <SolidColorBrush x:Key="MediumTextBrush" Color="{StaticResource MediumTextColor}" />
    <SolidColorBrush x:Key="LightTextBrush" Color="{StaticResource LightTextColor}" />

    <LinearGradientBrush x:Key="PressedBrush" StartPoint="0.5,0.042" EndPoint="0.5,0.971">
        <GradientStop Offset="0" Color="#4C000000" />
        <GradientStop Offset="1" Color="#26FFFFFF" />
        <GradientStop Offset="0.467" Color="#4C000000" />
        <GradientStop Offset="0.479" Color="#26FFFFFF" />
    </LinearGradientBrush>

    <!--  SolidColorBrush x:Key="WindowBackgroundBrush" Color="#FF595959" /  -->
    <LinearGradientBrush x:Key="WindowBackgroundBrush" StartPoint="0.7,0.042" EndPoint="0.5,0.971">
        <GradientStop Offset="0.0" Color="{StaticResource MainColor}" />
        <GradientStop Offset="0.9" Color="#FF333333" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="TitleBarBackgroundBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0.0" Color="{DynamicResource TitleBarDarkColor}" />
        <GradientStop Offset="1.0" Color="{DynamicResource TitleBarLightColor}" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="#FF333333" />

    <LinearGradientBrush x:Key="SelectedBackgroundBrush" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Offset="0" Color="{StaticResource TopGradientSelectColor}" />
        <GradientStop Offset="1" Color="{StaticResource BottomGradientSelectColor}" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="#888" />
    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="#FF787878" />
    <SolidColorBrush x:Key="DisabledBorderBrush" Color="#AAA" />

    <!--  EXPRESSION COMMON VALUES  -->

    <LinearGradientBrush x:Key="ShineBrush" StartPoint="0.5,0.042" EndPoint="0.5,0.971">
        <GradientStop Offset="0" Color="#59FFFFFF" />
        <GradientStop Offset="1" Color="#00FFFFFF" />
        <GradientStop Offset="0.467" Color="#26FFFFFF" />
        <GradientStop Offset="0.475" Color="#00FFFFFF" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="HoverShineBrush" StartPoint="0.5,0.042" EndPoint="0.5,0.971">
        <GradientStop Offset="0" Color="#4CFFFFFF" />
        <GradientStop Offset="1" Color="#26FFFFFF" />
        <GradientStop Offset="0.467" Color="#26FFFFFF" />
        <GradientStop Offset="0.475" Color="#00FFFFFF" />
        <GradientStop Offset="0.856" Color="#00FFFFFF" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="PressedBorderBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="#444" />
        <GradientStop Offset="1.0" Color="#888" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="DefaultedBorderBrush" Color="{StaticResource DefaultColor}" />
    <SolidColorBrush x:Key="SolidBorderBrush" Color="#888" />


    <!-- *************BRUSHES START************* -->
    <!--PROJECT TEMPLATE BRUSHES -->

    <!-- UNIVERSAL CONTROL BRUSHES -->
